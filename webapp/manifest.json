{
    "_version":"1.0.0",
    "sap.app":{
        "id": "Elearning",
        "type": "Elearning",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
        "version": "1.0.0"
        }
    },
    "sap.ui":{
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "Elearning.view.Login",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
          "minUI5Version": "1.60",
          "libs": {
            "sap.m": {}
          }
        },
        "models": {
          "i18n": {
            "type": "sap.ui.model.resource.ResourceModel",
            "settings": {
              "bundleName": "Elearning.i18n.i18n"
            }
          },
          "links": {
            "type": "sap.ui.model.json.JSONModel",
            "uri": "./model/links.json"
            }
        },
        "routing":{
            "config":{
              "routerClass":"sap.m.routing.Router",
              "viewType":"XML",
              "viewPath":"Elearning.view",
              "controlId":"app",
              "controlAggregation":"pages",
              "async":true
            },
            "routes":[
              {
                "pattern":"",
                "name":"loginpage",
                "target":"loginpage"
              },
              {
                "pattern":"login",
                "name":"loginpage",
                "target":"loginpage"
              },
              {
                "pattern":"home",
                "name":"homepage",
                "target":"homepage"
              },
              {
                "pattern":"mycourses",
                "name":"mycourses",
                "target":"mycourses"
              }
            ],
            "targets":{
              "loginpage":{
                "viewId":"loginpage",
                "viewName":"Login"
              },
              "homepage":{
                "viewId":"homepage",
                "viewName":"Dashboard"
              },
              "mycourses":{
                "viewId": "mycourses",
                "viewName":"MyCourses"
              }
            }
        }
    }
    
}